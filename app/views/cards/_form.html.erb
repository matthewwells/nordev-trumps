<div class="well">
	<%= form_for(@card) do |f| %>
	  <%= render(:partial => 'shared/errors', :locals => {:subject => @card}) %>
	
	
	  <div class="field">
	    <%= f.label :deck_id %>
	    <%= f.select :deck_id, Deck.decks_map, {}, class: 'input-xlarge' %>
	  </div>
	
	  <div class="field">
	    <%= f.label :name %>
	    <%= f.text_field :name %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val1 %>
	    <%= f.text_field :val1 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val2 %>
	    <%= f.text_field :val2 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val3 %>
	    <%= f.text_field :val3 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val4 %>
	    <%= f.text_field :val4 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val5 %>
	    <%= f.text_field :val5 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val6 %>
	    <%= f.text_field :val6 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val7 %>
	    <%= f.text_field :val7 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val8 %>
	    <%= f.text_field :val8 %>
	  </div>
	
	  <div class="field">
	    <%= f.label :val9 %>
	    <%= f.text_field :val9 %>
	  </div>
	
	  <div class="actions">
	    <%= f.submit 'Save Card', class: 'btn btn-primary' %>
	  </div>
	<% end %>
</div>